<h1>Admins::Users#index</h1>
<p>Find me in app/views/admins/users/index.html.erb</p>


<table>
  <tr>
    <th>ユーザー名（漢字）</th>
    <th>住所</th>
    <th>編集</th>
    <th>購入履歴一覧</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.name_kanji %></td>
    <td><%= user.address %></td>
    <td><%= link_to "編集",edit_admins_user_path(user)%></td>
    <td><%= link_to "購入履歴一覧",admins_user_path(user)%></td>
  </tr>
  <% end %>
</table>
